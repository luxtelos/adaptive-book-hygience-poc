# Logging Configuration
# Set to 'false' to disable all logging
VITE_LOGGING_ENABLED=true

# Log level: ERROR, WARN, INFO, DEBUG, TRACE
# Defaults to DEBUG in development, WARN in production
VITE_LOG_LEVEL=DEBUG

# Enable debugger statements (defaults to true in development)
VITE_ENABLE_DEBUGGER=true

# Enable production logging (defaults to false)
VITE_PROD_LOGGING=false

# Enable timestamps in logs (defaults to true)
VITE_LOG_TIMESTAMPS=true

# Enable colored output (defaults to true in development)
VITE_LOG_COLORS=true

# Log prefix for all messages
VITE_LOG_PREFIX=[BOOK-HYGIENE]

# QuickBooks API Configuration
VITE_QBO_PROXY_BASE_URL=https://your-proxy-server.com/api
VITE_QBO_REQUEST_TIMEOUT=30000
VITE_QBO_MAX_REQUESTS_PER_MINUTE=450
VITE_QBO_RETRY_DELAY_MS=1000
VITE_QBO_MAX_RETRIES=3
VITE_QBO_POST_LOGIN_REDIRECT=http://localhost:3000/oauth-callback

# Clerk Authentication
VITE_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key_here

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Application Configuration
VITE_APP_NAME=Adaptive Book Hygiene
VITE_APP_VERSION=1.0.0
